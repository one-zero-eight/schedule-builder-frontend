{
  "name": "schedule-builder-plugin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "login": "clasp login",
    "setup": "rimraf .clasp.json && mkdirp dist && clasp create --type sheets --title \"My React Project\" --rootDir ./dist && mv ./dist/.clasp.json ./.clasp.json && rimraf dist",
    "open": "clasp open --addon",
    "push": "clasp push",
    "build:dev": "vite build --mode development",
    "build": "tsc && vite build --mode production",
    "deploy:dev": "pnpm run build:dev && pnpm run push",
    "deploy": "pnpm run build && pnpm run push",
    "start": "pnpm run deploy:dev && pnpm run dev",
    "gen:api": "pnpm exec openapi-typescript --root-types --enum"
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.6",
    "@emotion/react": "^11.10.6",
    "@google/clasp": "^2.4.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@types/expect-puppeteer": "^5.0.0",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/node": "^20.11.30",
    "@types/puppeteer": "^5.4.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "aws-sdk": "^2.1106.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gas-client": "^1.2.0",
    "gas-types-detailed": "^1.1.2",
    "openapi-fetch": "^0.14.0",
    "openapi-react-query": "^0.5.0",
    "openapi-typescript": "^7.9.1",
    "openapi-typescript-helpers": "^0.0.15",
    "postcss": "^8.4.38",
    "postcss-preset-env": "^9.5.4",
    "prettier": "^2.7.0",
    "prop-types": "^15.8.1",
    "puppeteer": "^14.3.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.18.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-singlefile": "^2.0.1",
    "vite-plugin-static-copy": "^2.3.2"
  }
}
